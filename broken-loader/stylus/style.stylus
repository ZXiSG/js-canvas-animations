@import url("http://fonts.googleapis.com/css?family=Source+Code+Pro");
*,
*:before,
*:after
  box-sizing border-box

body
  background-color #111
  color #fff
  font-family "Source Code Pro", mono
  font-weight 400
  padding 20px 40px
  font-smoothing antialiased

.loader
  margin-bottom 1em

.loader-increment
  color #00b200
